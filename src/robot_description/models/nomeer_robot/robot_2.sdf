<?xml version="1.0"?>

<sdf version="1.9">

    <!-- nomeer_robot model -->
    <model name='nomeer_robot' canonical_link='base_link'>

        <static>false</static>
        <self_collide>false</self_collide>


        <!-- links-->
        <!-- base_footprint link -->
        <!--link
        name="base_footprint" /-->

        <!-- base_link -->
        <link name="base_link">0.0 <pose relative_to='__model__'>0 0 0.05 0 0 0</pose>

            <inertial>
                <mass>3.0</mass>
                <inertia>
                    <ixx>0.0054</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.0252</iyy>
                    <iyz>0</iyz>
                    <izz>0.0252</izz>
                </inertia>
            </inertial>

            <visual
                name='visual'>
                <geometry>
                    <box>
                        <size>0.2 0.1 0.04</size>
                    </box>
                </geometry>
                <material> <!-- blue color -->
                    <ambient>0.0 0.0 1.0 1</ambient>
                    <diffuse>0.0 0.0 1.0 1</diffuse>
                    <specular>0.0 0.0 1.0 1</specular>
                </material>
            </visual>

            <collision
                name='collision'>
                <geometry>
                    <box>
                        <size>0.2 0.1 0.04</size>
                    </box>
                </geometry>
            </collision>

        </link>

        <!-- front_right_wheel_link-->
        <link name="front_right_wheel_link">

            <pose relative_to="base_link">0.075 -0.05 -0.025 1.5707 0 0</pose>

            <inertial>
                <mass>0.05</mass>
                <inertia>
                    <ixx>0.00003331</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.0000204</iyy>
                    <iyz>0</iyz>
                    <izz>0.0000204</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.025</radius>
                        <length>0.02</length>
                    </cylinder>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.025</radius>
                        <length>0.02</length>
                    </cylinder>
                </geometry>
            </collision>

        </link>


        <!-- front_left_wheel_link-->
        <link name="front_left_wheel_link">

            <pose relative_to="base_link">0.075 0.05 -0.025 1.5707 0 0</pose>

            <inertial>
                <mass>0.05</mass>
                <inertia>
                    <ixx>0.00003331</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.0000204</iyy>
                    <iyz>0</iyz>
                    <izz>0.0000204</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.025</radius>
                        <length>0.02</length>
                    </cylinder>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.025</radius>
                        <length>0.01</length>
                    </cylinder>
                </geometry>
            </collision>

        </link>

        <!-- steering_front_right_link -->
        <link name="steering_front_right_link">

            <pose relative_to="base_link">0.075 -0.05 -0.025 1.5707 0 0</pose>

            <inertial>
                <mass>0.005</mass>
                <inertia>
                    <ixx>0.0000018</ixx>
                    <iyy>0.0000018</iyy>
                    <izz>0.0000018</izz>
                </inertia>
            </inertial>

        </link>

        <!-- steering_front_left_link -->
        <link name="steering_front_left_link">

            <pose relative_to="base_link">0.075 0.05 -0.025 1.5707 0 0</pose>

            <inertial>
                <mass>0.005</mass>
                <inertia>
                    <ixx>0.0000018</ixx>
                    <iyy>0.0000018</iyy>
                    <izz>0.0000018</izz>
                </inertia>
            </inertial>

        </link>


        <!-- rear_right_wheel_link-->
        <link name="rear_right_wheel_link">

            <pose relative_to="base_link">-0.075 0.05 -0.025 1.5707 0 0</pose>

            <inertial>
                <mass>0.05</mass>
                <inertia>
                    <ixx>0.00003331</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.0000204</iyy>
                    <iyz>0</iyz>
                    <izz>0.0000204</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.025</radius>
                        <length>0.02</length>
                    </cylinder>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.025</radius>
                        <length>0.02</length>
                    </cylinder>
                </geometry>
            </collision>

        </link>


        <!-- rear_left_wheel_link-->
        <link name="rear_left_wheel_link">

            <pose relative_to="base_link">-0.075 -0.05 -0.025 1.5707 0 0</pose>

            <inertial>
                <mass>0.05</mass>
                <inertia>
                    <ixx>0.00003331</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.0000204</iyy>
                    <iyz>0</iyz>
                    <izz>0.0000204</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.025</radius>
                        <length>0.02</length>
                    </cylinder>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.025</radius>
                        <length>0.02</length>
                    </cylinder>
                </geometry>
            </collision>

        </link>

        <!-- camera_link-->
        <link name="camera_link">

            <pose relative_to="base_link">0.075 0 0.05 0 0 0</pose>

            <visual name='visual'>
                <geometry>
                    <box>
                        <size>0.0125 0.0125 0.0125</size>
                    </box>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <box>
                        <size>0.0125 0.0125 0.0125</size>
                    </box>
                </geometry>
            </collision>

            <sensor name="camera" type="camera">
                <pose relative_to='camera_link'>0 0 0 0 0 0</pose>
                <camera>
                    <horizontal_fov>1.047</horizontal_fov>
                    <image>
                        <width>320</width>
                        <height>240</height>
                    </image>
                    <clip>
                        <near>0.1</near>
                        <far>100</far>
                    </clip>
                </camera>
                <always_on>1</always_on>
                <update_rate>10</update_rate>
                <visualize>true</visualize>
            </sensor>

        </link>

        <!-- lidar_link-->
        <link name="lidar_link">

            <pose relative_to="base_link">0.075 0 0.02125 0 0 0</pose>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.0125</radius>
                        <length>0.0125</length>
                    </cylinder>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.0125</radius>
                        <length>0.0125</length>
                    </cylinder>
                </geometry>
            </collision>

            <sensor name="lidar" type="gpu_lidar">
                <lidar>
                    <scan>
                        <horizontal>
                            <samples>400</samples>
                            <resolution>1</resolution>
                            <min_angle>-1.5707</min_angle>
                            <max_angle>1.5707</max_angle>
                        </horizontal>
                    </scan>
                    <range>
                        <min>0.15</min>
                        <max>25</max>
                        <resolution>0.01</resolution>
                    </range>
                </lidar>
                <gz_frame_id>lidar_link</gz_frame_id>
                <alwaysOn>1</alwaysOn>
                <visualize>true</visualize>
                <update_rate>10</update_rate>
            </sensor>

        </link>


        <!-- joins -->
        <!-- front_right_wheel_joint -->
        <joint name='front_right_wheel_joint' type='revolute'>
            <pose relative_to='front_right_wheel_link' />
            <parent>base_link</parent>
            <child>front_right_wheel_link</child>
            <axis>
                <xyz>0 1 0</xyz>
                <limit>
                    <lower>-inf</lower>
                    <upper>inf</upper>
                </limit>
                <dynamics>
                    <spring_reference>0</spring_reference>
                    <spring_stiffness>0</spring_stiffness>
                </dynamics>
            </axis>
        </joint>

        <!-- front_left_wheel_joint -->
        <joint name='front_left_wheel_joint' type='revolute'>
            <pose relative_to='front_left_wheel_link' />
            <parent>base_link</parent>
            <child>front_left_wheel_link</child>
            <axis>
                <xyz>0 1 0</xyz>
                <limit>
                    <lower>-inf</lower>
                    <upper>inf</upper>
                </limit>
                <dynamics>
                    <spring_reference>0</spring_reference>
                    <spring_stiffness>0</spring_stiffness>
                </dynamics>
            </axis>
        </joint>

        <!-- steering_front_right_joint -->
        <joint name="steering_front_right_joint" type="revolute">
            <pose relative_to="steering_front_right_link" />
            <parent>base_link</parent>
            <child>steering_front_right_link</child>
            <axis>
                <xyz>0 0 1</xyz>
                <limit>
                    <lower>-0.6</lower>
                    <upper>0.6</upper>
                    <velocity>1.0</velocity>
                    <effort>25</effort>
                </limit>
                <dynamics>
                    <spring_reference>0</spring_reference>
                    <spring_stiffness>0</spring_stiffness>
                </dynamics>
            </axis>
        </joint>

        <!-- steering_front_left_joint -->
        <joint name="steering_front_left_joint" type="revolute">
            <pose relative_to="steering_front_left_link" />
            <parent>base_link</parent>
            <child>steering_front_left_link</child>
            <axis>
                <xyz>0 0 1</xyz>
                <limit>
                    <lower>-0.6</lower>
                    <upper>0.6</upper>
                    <velocity>1.0</velocity>
                    <effort>25</effort>
                </limit>
                <dynamics>
                    <spring_reference>0</spring_reference>
                    <spring_stiffness>0</spring_stiffness>
                </dynamics>
            </axis>
        </joint>

        <!-- rear_right_wheel_joint -->
        <joint name='rear_right_wheel_joint' type='revolute'>
            <pose relative_to='rear_right_wheel_link' />
            <parent>base_link</parent>
            <child>rear_right_wheel_link</child>
            <axis>
                <xyz>0 1 0</xyz>
                <limit>
                    <lower>-inf</lower>
                    <upper>inf</upper>
                </limit>
                <dynamics>
                    <spring_reference>0</spring_reference>
                    <spring_stiffness>0</spring_stiffness>
                </dynamics>
            </axis>
        </joint>

        <!-- rear_left_wheel_joint -->
        <joint name='rear_left_wheel_joint' type='revolute'>
            <pose relative_to='rear_left_wheel_link' />
            <parent>base_link</parent>
            <child>rear_left_wheel_link</child>
            <axis>
                <xyz>0 1 0</xyz>
                <limit>
                    <lower>-inf</lower>
                    <upper>inf</upper>
                </limit>
                <dynamics>
                    <spring_reference>0</spring_reference>
                    <spring_stiffness>0</spring_stiffness>
                </dynamics>
            </axis>
        </joint>

        <!-- camera_joint -->
        <joint name="camera_joint" type="fixed">
            <pose relative_to='camera_link' />
            <parent>base_link</parent>
            <child>camera_link</child>
        </joint>

        <!-- lidar_joint -->
        <joint name="lidar_joint" type="fixed">
            <pose relative_to='lidar_link' />
            <parent>base_link</parent>
            <child>lidar_link</child>
        </joint>


        <!-- plugins -->
        <!-- joint_controller plugin-->
        <!--plugin
            filename="gz-sim-joint-controller-system"
            name="gz::sim::systems::JointController">
            <joint_name>front_left_wheel_joint</joint_name>
            <joint_name>rear_left_wheel_joint</joint_name>
            <joint_name>rear_right_wheel_joint</joint_name>
            <joint_name>front_right_wheel_joint</joint_name>
            <topic>drive</topic>
        </plugin-->

        <!-- ackermann plugin -->
        <plugin
            filename="gz-sim-ackermann-steering-system"
            name="gz::sim::systems::AckermannSteering">
            <left_joint>rear_left_wheel_joint</left_joint>
            <left_joint>front_left_wheel_joint</left_joint>
            <right_joint>rear_right_wheel_joint</right_joint>
            <right_joint>front_right_wheel_joint</right_joint>
            <left_steering_joint>steering_front_left_joint</left_steering_joint>
            <right_steering_joint>steering_front_right_joint</right_steering_joint>
            <kingpin_width>0.13</kingpin_width>
            <steering_limit>0.4</steering_limit>
            <wheel_base>0.23521</wheel_base>
            <wheel_separation>0.164</wheel_separation>
            <wheel_radius>0.0345</wheel_radius>
            <min_velocity>-10.0</min_velocity>
            <max_velocity>10.0</max_velocity>
            <min_acceleration>-10</min_acceleration>
            <max_acceleration>10</max_acceleration>
            <topic>/cmd_vel</topic>
        </plugin>

        <!-- joint_state_publisher plugin -->
        <plugin
            filename="gz-sim-joint-state-publisher-system"
            name="gz::sim::systems::JointStatePublisher">
            <joint_name>front_left_wheel_joint</joint_name>
            <joint_name>rear_left_wheel_joint</joint_name>
            <joint_name>rear_right_wheel_joint</joint_name>
            <joint_name>front_right_wheel_joint</joint_name>
            <joint_name>steering_front_left_joint</joint_name>
            <joint_name>steering_front_right_joint</joint_name>
        </plugin>

    </model>


</sdf>