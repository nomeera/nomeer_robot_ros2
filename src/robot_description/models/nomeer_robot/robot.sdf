<?xml version="1.0"?>

<sdf version="1.9">

    <!-- nomeer_robot model -->
    <model name='nomeer_robot' canonical_link='base_link'>

        <static>false</static>
        <self_collide>false</self_collide>

        <!-- links-->   
        <!-- base_footprint -->
        <link name="base_footprint" />

        <!-- base_link -->
        <link name="base_link">

            <pose relative_to='__model__'>0 0 0.3 0 0 0</pose>

            <inertial>
                <mass>8.0</mass>
                <inertia>
                    <ixx>0.426667</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.24</iyy>
                    <iyz>0</iyz>
                    <izz>0.666667</izz>
                </inertia>
            </inertial>

            <visual
                name='visual'>
                <geometry>
                    <box>
                        <size>0.8 0.6 0.5</size>
                    </box>
                </geometry>
                <material> <!-- blue color -->
                    <ambient>0.0 0.0 1.0 1</ambient>
                    <diffuse>0.0 0.0 1.0 1</diffuse>
                    <specular>0.0 0.0 1.0 1</specular>
                </material>
            </visual>

            <collision
                name='collision'>
                <geometry>
                    <box>
                        <size>0.8 0.6 0.5</size>
                    </box>
                </geometry>
            </collision>

        </link>

        <!-- body_link-->
        <link name="body_link">

            <pose relative_to="base_link">-0.3125 0 0.75 0 1.5708 0</pose>

            <inertial>
                <mass>2.0</mass>
                <inertia>
                    <ixx>0.166667</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.06</iyy>
                    <iyz>0</iyz>
                    <izz>0.226667</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <box>
                        <size>1.0 0.6 0.175</size>
                    </box>
                </geometry>
                <material> <!-- blue color -->
                    <ambient>0.0 0.0 1.0 1</ambient>
                    <diffuse>0.0 0.0 1.0 1</diffuse>
                    <specular>0.0 0.0 1.0 1</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <box>
                        <size>1.0 0.6 0.175</size>
                    </box>
                </geometry>
            </collision>

        </link>

        <!-- right_wheel arbitrary frame -->
        <frame name="right_wheel_frame" attached_to='base_link'>
            <pose>-0.25 0.25 -0.25 1.5707 0 0</pose>
        </frame>
        <!-- right_wheel_link-->
        <link name="right_wheel_link">

            <pose relative_to="right_wheel_frame" />

            <inertial>
                <mass>0.4</mass>
                <inertia>
                    <ixx>0.001213</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.001213</iyy>
                    <iyz>0</iyz>
                    <izz>0.002</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.08</length>
                    </cylinder>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision
                name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.08</length>
                    </cylinder>
                </geometry>
            </collision>

        </link>

        <!-- left_wheel arbitrary frame -->
        <frame name="left_wheel_frame" attached_to='base_link'>
            <pose>-0.25 -0.25 -0.25 1.5707 0 0</pose>
        </frame>
        <!-- left_wheel_link-->
        <link name="left_wheel_link">

            <pose relative_to="left_wheel_frame" />

            <inertial>
                <mass>0.4</mass>
                <inertia>
                    <ixx>0.001213</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.001213</iyy>
                    <iyz>0</iyz>
                    <izz>0.002</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.08</length>
                    </cylinder>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.08</length>
                    </cylinder>
                </geometry>
            </collision>

        </link>

        <!-- caster arbitrary frame -->
        <frame name="caster_frame" attached_to='base_link'>
            <pose>0.25 0 -0.28 0 0 0</pose>
        </frame>
        <!-- caster_link-->
        <link name="caster_link">

            <pose relative_to="caster_frame" />

            <inertial>
                <mass>0.3</mass>
                <inertia>
                    <ixx>0.0012</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.0012</iyy>
                    <iyz>0</iyz>
                    <izz>0.0012</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <sphere>
                        <radius>0.06</radius>
                    </sphere>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <sphere>
                        <radius>0.06</radius>
                    </sphere>
                </geometry>
            </collision>

        </link>

        <!-- camera_link-->
        <link name="camera_link">

            <pose relative_to="base_link">-0.225 0 1.05 0 0 0</pose>

            <visual name='visual'>
                <geometry>
                    <box>
                        <size>0.05 0.05 0.05</size>
                    </box>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <box>
                        <size>0.05 0.05 0.05</size>
                    </box>
                </geometry>
            </collision>

            <sensor name="camera" type="camera">
                <pose relative_to='camera_link'>0 0 0 0 0 0</pose>
                <topic>camera</topic>
                <camera>
                    <horizontal_fov>1.047</horizontal_fov>
                    <image>
                        <width>320</width>
                        <height>240</height>
                    </image>
                    <clip>
                        <near>0.1</near>
                        <far>100</far>
                    </clip>
                </camera>
                <always_on>1</always_on>
                <update_rate>10</update_rate>
                <visualize>true</visualize>
            </sensor>

        </link>

        <!-- lidar_link-->
        <link name="lidar_link">

            <pose relative_to="base_link">0.4 0 -0.05 0 0 0</pose>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.05</length>
                    </cylinder>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.05</length>
                    </cylinder>
                </geometry>
            </collision>

            <sensor name='gpu_lidar' type='gpu_lidar'>
                <pose relative_to='lidar_link'>0 0 0 0 0 0</pose>
                <topic>lidar</topic>
                <update_rate>10</update_rate>
                <ray>
                    <scan>
                        <horizontal>
                            <samples>400</samples>
                            <resolution>1</resolution>
                            <min_angle>-1.5707</min_angle>
                            <max_angle>1.5707</max_angle>
                        </horizontal>
                        <vertical>
                            <samples>1</samples>
                            <resolution>0.01</resolution>
                            <min_angle>0</min_angle>
                            <max_angle>0</max_angle>
                        </vertical>
                    </scan>
                    <range>
                        <min>0.15</min>
                        <max>25.0</max>
                        <resolution>0.01</resolution>
                    </range>
                </ray>
                <gz_frame_id>lidar_link</gz_frame_id>
                <always_on>1</always_on>
                <visualize>true</visualize>
            </sensor>

        </link>

        <!-- joins -->
        <!-- base_joint -->
        <joint name="base_joint" type="fixed">
            <pose relative_to='base_link' />
            <parent>base_link</parent>
            <child>base_footprint</child>
        </joint>

        <!-- body_joint -->
        <joint name="body_joint" type="fixed">
            <pose relative_to='body_link' />
            <parent>base_link</parent>
            <child>body_link</child>
        </joint>

        <!-- right_wheel_joint -->
        <joint name='right_wheel_joint' type='revolute'>
            <pose relative_to='right_wheel_link' />
            <parent>base_link</parent>
            <child>right_wheel_link</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative
                    infinity-->
                    <upper>1.79769e+308</upper>     <!--positive
                    infinity-->
                </limit>
            </axis>
        </joint>

        <!-- left_wheel_joint -->
        <joint name='left_wheel_joint' type='revolute'>
            <pose relative_to='left_wheel_link' />
            <parent>base_link</parent>
            <child>left_wheel_link</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative
                    infinity-->
                    <upper>1.79769e+308</upper>     <!--positive
                    infinity-->
                </limit>
            </axis>
        </joint>

        <!-- caster_joint -->
        <joint name="caster_joint" type='ball'>
            <pose relative_to="caster_link" />
            <parent>base_link</parent>
            <child>caster_link</child>
        </joint>

        <!-- camera_joint -->
        <joint name="camera_joint" type="fixed">
            <pose relative_to='camera_link' />
            <parent>base_link</parent>
            <child>camera_link</child>
        </joint>

        <!-- lidar_joint -->
        <joint name="lidar_joint" type="fixed">
            <pose relative_to='lidar_link' />
            <parent>base_link</parent>
            <child>lidar_link</child>
        </joint>


        <!-- plugins -->
        <!-- diff_drive_plugin -->
        <plugin
            filename="gz-sim-diff-drive-system"
            name="gz::sim::systems::DiffDrive">
            <left_joint>left_wheel_joint</left_joint>
            <right_joint>right_wheel_joint</right_joint>
            <wheel_separation>0.5</wheel_separation>
            <wheel_radius>0.1</wheel_radius>
            <topic>cmd_vel</topic>
            <odom_topic>/odom</odom_topic>
            <frame_id>odom</frame_id>
            <child_frame_id>base_link</child_frame_id>
        </plugin>

        <!-- joint state publisher plugin -->
        <plugin
            filename="gz-sim-joint-state-publisher-system"
            name="gz::sim::systems::JointStatePublisher">
            <joint_name>base_joint</joint_name>
            <joint_name>body_joint</joint_name>
            <joint_name>right_wheel_joint</joint_name>
            <joint_name>left_wheel_joint</joint_name>
            <joint_name>caster_joint</joint_name>
            <joint_name>camera_joint</joint_name>
            <joint_name>lidar_joint</joint_name>
        </plugin>

        <plugin
            filename="gz-sim-odometry-publisher-system"
            name="gz::sim::systems::OdometryPublisher">
            <odom_frame>odom</odom_frame>
            <odom_covariance_topic>odometry_with_covariance</odom_covariance_topic>
            <tf_topic>tf</tf_topic>
            <robot_base_frame>base_footprint</robot_base_frame>
            <odom_topic>odom</odom_topic>
        </plugin>

    </model>


</sdf>