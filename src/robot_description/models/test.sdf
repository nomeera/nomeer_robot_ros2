<?xml version="1.0" ?>
<sdf version="1.8">
    <world name="car_world">
        <physics name="1ms" type="ignored">
            <max_step_size>0.001</max_step_size>
            <real_time_factor>1.0</real_time_factor>
        </physics>
        <plugin
            filename="libignition-gazebo-physics-system.so"
            name="ignition::gazebo::systems::Physics">
        </plugin>
        <plugin
            filename="libignition-gazebo-user-commands-system.so"
            name="ignition::gazebo::systems::UserCommands">
        </plugin>
        <plugin
            filename="libignition-gazebo-scene-broadcaster-system.so"
            name="ignition::gazebo::systems::SceneBroadcaster">
        </plugin>

        <light type="directional" name="sun">
            <cast_shadows>true</cast_shadows>
            <pose>0 0 10 0 0 0</pose>
            <diffuse>0.8 0.8 0.8 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
            <attenuation>
                <range>1000</range>
                <constant>0.9</constant>
                <linear>0.01</linear>
                <quadratic>0.001</quadratic>
            </attenuation>
            <direction>-0.5 0.1 -0.9</direction>
            
        </light>

                <model name="ground_plane">
            <static>true</static>
            <link name="link">
                <collision name="collision">
                <geometry>
                    <plane>
                    <normal>0 0 1</normal>
                    </plane>
                </geometry>
                </collision>
                <visual name="visual">
                <geometry>
                    <plane>
                    <normal>0 0 1</normal>
                    <size>100 100</size>
                    </plane>
                </geometry>
                <material>
                    <ambient>0.8 0.8 0.8 1</ambient>
                    <diffuse>0.8 0.8 0.8 1</diffuse>
                    <specular>0.8 0.8 0.8 1</specular>
                </material>
                </visual>
            </link>
        </model>


    <!-- our nomeer_robot model -->
    <model name='nomeer_robot' canonical_link='base'>

        <pose relative_to='world'>0 0 0 0 0 3.141592654</pose>
        <static>false</static>

        <!-- links-->
        <!-- odom -->
        <link name="odom">
            <pose relative_to="__model__">0 0 0 0 0 0</pose>
        </link>

        <!-- base -->
        <link name="base">

            <pose relative_to="odom">0 0 0.35 0 0 0</pose>

            <inertial>
                <mass>8.0</mass>
                <inertia>
                    <ixx>0.426667</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.24</iyy>
                    <iyz>0</iyz>
                    <izz>0.666667</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <box>
                        <size>0.8 0.6 0.5</size>
                    </box>
                </geometry>
                <material> <!-- blue color -->
                    <ambient>0.0 0.0 1.0 1</ambient>
                    <diffuse>0.0 0.0 1.0 1</diffuse>
                    <specular>0.0 0.0 1.0 1</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <box>
                        <size>0.8 0.6 0.5</size>
                    </box>
                </geometry>
            </collision>

        </link>

        <!-- body-->
        <link name="body">

            <pose relative_to="base">-0.3125 0 0.75 0 1.5708 0</pose>

            <inertial>
                <mass>2.0</mass>
                <inertia>
                    <ixx>0.166667</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.06</iyy>
                    <iyz>0</iyz>
                    <izz>0.226667</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <box>
                        <size>1.0 0.6 0.175</size>
                    </box>
                </geometry>
                <material> <!-- blue color -->
                    <ambient>0.0 0.0 1.0 1</ambient>
                    <diffuse>0.0 0.0 1.0 1</diffuse>
                    <specular>0.0 0.0 1.0 1</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <box>
                        <size>1.0 0.6 0.175</size>
                    </box>
                </geometry>
            </collision>

        </link>

        <!-- right_wheel-->
        <link name="right_wheel">

            <pose relative_to="base">-0.25 0.25 -0.25 1.5707 0 0</pose>

            <inertial>
                <mass>0.4</mass>
                <inertia>
                    <ixx>0.001213</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.001213</iyy>
                    <iyz>0</iyz>
                    <izz>0.002</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.08</length>
                    </cylinder>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.08</length>
                    </cylinder>
                </geometry>
            </collision>

        </link>

        <!-- left_wheel-->
        <link name="left_wheel">

            <pose relative_to="base">-0.25 -0.25 -0.25 1.5707 0 0</pose>

            <inertial>
                <mass>0.4</mass>
                <inertia>
                    <ixx>0.001213</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.001213</iyy>
                    <iyz>0</iyz>
                    <izz>0.002</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.08</length>
                    </cylinder>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.1</radius>
                        <length>0.08</length>
                    </cylinder>
                </geometry>
            </collision>

        </link>

        <!-- caster-->
        <link name="caster">

            <pose relative_to="base">0.25 0 -0.28 0 0 0</pose>

            <inertial>
                <mass>0.3</mass>
                <inertia>
                    <ixx>0.0012</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.0012</iyy>
                    <iyz>0</iyz>
                    <izz>0.0012</izz>
                </inertia>
            </inertial>

            <visual name='visual'>
                <geometry>
                    <sphere>
                        <radius>0.06</radius>
                    </sphere>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <sphere>
                        <radius>0.06</radius>
                    </sphere>
                </geometry>
            </collision>

        </link>

        <!-- camera-->
        <link name="camera">

            <pose relative_to="base">-0.225 0 1.05 0 0 0</pose>

            <visual name='visual'>
                <geometry>
                    <box>
                        <size>0.05 0.05 0.05</size>
                    </box>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <box>
                        <size>0.05 0.05 0.05</size>
                    </box>
                </geometry>
            </collision>

        </link>

        <!-- lidar-->
        <link name="lidar">

            <pose relative_to="base">0.4 0 -0.05 0 0 0</pose>

            <visual name='visual'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.05</length>
                    </cylinder>
                </geometry>
                <material> <!-- white color -->
                    <ambient>1.0 1.0 1.0 1.0</ambient>
                    <diffuse>1.0 1.0 1.0 1.0</diffuse>
                    <specular>1.0 1.0 1.0 1.0</specular>
                </material>
            </visual>

            <collision name='collision'>
                <geometry>
                    <cylinder>
                        <radius>0.05</radius>
                        <length>0.05</length>
                    </cylinder>
                </geometry>
            </collision>

        </link>


        <!-- joins -->
        <!-- base_joint -->
        <joint name="base_joint" type="fixed">
            <pose relative_to='base' />
            <parent>odom</parent>
            <child>base</child>
        </joint>

        <!-- body_joint -->
        <joint name="body_joint" type="fixed">
            <pose relative_to='body' />
            <parent>base</parent>
            <child>body</child>
        </joint>

        <!-- right_wheel_joint -->
        <joint name='right_wheel_joint' type='revolute'>
            <pose relative_to='right_wheel' />
            <parent>base</parent>
            <child>right_wheel</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative
                    infinity-->
                    <upper>1.79769e+308</upper>     <!--positive
                    infinity-->
                </limit>
            </axis>
        </joint>

        <!-- left_wheel_joint -->
        <joint name='left_wheel_joint' type='revolute'>
            <pose relative_to='left_wheel' />
            <parent>base</parent>
            <child>left_wheel</child>
            <axis>
                <xyz expressed_in='__model__'>0 1 0</xyz>
                <limit>
                    <lower>-1.79769e+308</lower>    <!--negative
                    infinity-->
                    <upper>1.79769e+308</upper>     <!--positive
                    infinity-->
                </limit>
            </axis>
        </joint>

        <!-- caster -->
        <joint name='caster_joint' type='ball'>
            <parent>base</parent>
            <child>caster</child>
        </joint>

        <!-- camera_joint -->
        <joint name="camera_joint" type="fixed">
            <pose relative_to='camera' />
            <parent>base</parent>
            <child>camera</child>
        </joint>

        <!-- lidar_joint -->
        <joint name="lidar_joint" type="fixed">
            <pose relative_to='lidar' />
            <parent>base</parent>
            <child>lidar</child>
        </joint>


        <!-- plugins -->
        <!-- diff_drive_plugin -->
        <plugin
            filename="libignition-gazebo-diff-drive-system.so"
            name="ignition::gazebo::systems::DiffDrive">
            <left_joint>left_wheel_joint</left_joint>
            <right_joint>right_wheel_joint</right_joint>
            <wheel_separation>0.5</wheel_separation>
            <wheel_radius>0.1</wheel_radius>
            <odom_publish_frequency>1</odom_publish_frequency>
            <topic>cmd_vel</topic>
        </plugin>

    </model>


    </world>
</sdf>